name = CEL
description = A responsive design for the CEL website
core = 7.x
version = 7.x-1.0
base theme = wl

; Regions
regions[navigation]     = 'Primary Navigation'
regions[secondary_navigation] = 'Secondary Navigation'
regions[header]         = 'Top Bar'
regions[help]           = 'Help'
regions[content]        = 'Content'
regions[social-media]   = 'Social Media'
regions[footer]         = 'Footer'

; CSS
; Stylesheets
; stylesheets[all][] = css/bootstrap.css
; stylesheets[all][] = css/bootstrap-glyphicons.css
; stylesheets[all][] = css/overrides.css
stylesheets[all][] = css/jquery-jvectormap-1.2.2.css
stylesheets[all][] = css/style.css

; Scripts
; scripts[] = js/bootstrap.js
scripts[] = js/froogaloop.min.js
scripts[] = js/URI.js
scripts[] = js/xregexp-min.js
scripts[] = js/unicode-base.js
scripts[] = js/jquery-jvectormap-1.2.2.min.js
scripts[] = js/views-json-helper.js
scripts[] = js/partner-maps.js

;Breakpoints
breakpoints[mobile] = (min-width: 0px)
breakpoints[tablet] = (min-width: 768px)
breakpoints[med-desktop] = (min-width: 992px)
breakpoints[large-desktop] = (min-width: 1200px)

; Panels layouts. You can place multiple layouts under the "layouts" folder.
plugins[panels][layouts] = layouts