name = Whereâ€™s Lucian base theme
description = Built on top of the Bootstrap base theme, to be used with Panels.
core = 7.x
version = 7.x-2.0
package = WheresLucian.com
project = wl
project status url = "http://whereslucian.com/drupal"

; Regions
regions[navigation]     = 'Navigation'
regions[header]         = 'Top Bar'
regions[help]           = 'Help'
regions[content]        = 'Content'
regions[footer]         = 'Footer'

settings[toggle_name] = 0
settings[toggle_search] = 1

; Settings for Bootstrap
settings[cdn_bootstrap] = 1
settings[cdn_bootstrap_version] = 3.0.0-rc1

; Settings for the jQuery CDN
settings[cdn_jquery] = 1
settings[cdn_jquery_version] = 1.10.1
settings[cdn_jquery_migrate] = 1
settings[cdn_jquery_migrate_version] = 1.2.1

; CSS
stylesheets[all][] = css/bootstrap-glyphicons.css
stylesheets[all][] = css/overrides.css

; Scripts
scripts[] = js/spin.js
scripts[] = js/jquery.fitvids.js

